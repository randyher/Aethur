<!DOCTYPE html>
<html>
  <head>
    <title>HalfTilt</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
      <nav class="navbar navbar-expand-lg navbar-light bg-light">
  <a class="navbar-brand" href="#">Navbar</a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
    <div class="navbar-nav">
      <a class="nav-item nav-link active" href="#">Home <span class="sr-only">(current)</span></a>
      <a class="nav-item nav-link" href="/videos/list/">Videos</a>
      <a class="nav-item nav-link" href="/blogs/list/">Blogs</a>
      <a class="nav-item nav-link" href="/teams/list">Teams</a>
       <a class="nav-item nav-link" href="/users/<%=current_user.id%>">Account</a>
       <%= link_to "account", user_path(current_user), class: "nav-item nav-link" %>
    </div>
  </div>
  <form class="form-inline">
      <a class="btn btn-outline-primary" href="/users/sign_in" role="button">
      Sign Out
      </a>
     
 </form>
</nav>
      <div class="container">
          <p class="notice"><%= notice %></p>
          <p class="alert"><%= alert %></p>
          <%= yield %>
      </div>

  </body>
</html>
